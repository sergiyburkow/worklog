{
  "name": "worklog",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "api:dev": "npm run start:dev --workspace=packages/api",
    "web:dev": "npm run dev --workspace=packages/web",
    "mobile:dev": "npm run start --workspace=packages/mobile",
    "test": "npm run test --workspaces",
    "generate-certs": "chmod +x scripts/generate-certs.sh && ./scripts/generate-certs.sh"
  },
  "keywords": [
    "project-management",
    "task-tracking",
    "worklog"
  ],
  "author": "",
  "license": "ISC",
  "description": "Comprehensive project and task management system",
  "devDependencies": {
    "@types/passport-jwt": "^4.0.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "compression": "^1.8.0",
    "expo-constants": "~17.0.7",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "framer-motion": "^12.4.10",
    "nest-winston": "^1.10.2",
    "react-icons": "^5.5.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "winston": "^3.17.0"
  }
}
